{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"inputs": [
				"$TURBO_DEFAULT$",
				".env*"
			],
			"outputs": [
				"dist/**"
			]
		},
		"build:es": {
			"dependsOn": [
				"^build:es"
			],
			"outputs": [
				"dist/es/**"
			]
		},
		"build:cjs": {
			"dependsOn": [
				"^build:cjs"
			],
			"outputs": [
				"dist/lib/**"
			]
		},
		"build:umd": {
			"dependsOn": [
				"^build:umd"
			],
			"outputs": [
				"dist/*.min.js"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		},
		"lint:fix": {
			"outputs": []
		},
		"check-types": {
			"dependsOn": [
				"^check-types"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
